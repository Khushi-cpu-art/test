{
  "name": "selenium-mochawesome-test",
  "version": "1.0.0",
  "description": "Automated Selenium tests with Mochawesome HTML reports",
  "main": "test.js",
  "scripts": {
    "test": "mocha test.js --reporter mochawesome --reporter-options reportDir=mochawesome-report,reportFilename=report,html=true,json=false",
    "postinstall": "node -e \"require('chromedriver-autoinstall').install()\""
  },
  "keywords": [
    "selenium",
    "mocha",
    "mochawesome",
    "testing",
    "automation",
    "chromedriver"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "chromedriver-autoinstall": "^1.1.1",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "selenium-webdriver": "^4.16.0"
  }
}
