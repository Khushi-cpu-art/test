{
  "name": "selenium-mochawesome-test",
  "version": "1.0.0",
  "description": "Selenium test with mochawesome report and embedded screenshots",
  "main": "test.js",
  "scripts": {
    "test": "mocha test.js --reporter mochawesome --reporter-options reportDir=mochawesome-report,reportFilename=mochawesome,inlineAssets=true"
  },
  "devDependencies": {
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "selenium-webdriver": "^4.11.0",
    "chromedriver": "^139.0.0"
  }
}
